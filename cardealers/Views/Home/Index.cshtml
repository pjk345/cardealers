@model cardealers.ViewModels.HomeVM


    <h2>@Model.Title</h2>

    @foreach (var car in Model.Cars)
    {
        <div>
            <h2>@car.Brand</h2>
            <h2>@car.Model</h2>
            <h3>@car.ProductionYear</h3>
            <h3>@car.Mileage</h3>
            <h4>@car.Capacity</h4>
            <h4>@car.Power</h4>
            <h5>@car.Price.ToString("c")</h5>
            <p>@car.Description</p>
        </div>
    }
